  <devices>
    <family Dfamily="MIMXRT1187" Dvendor="NXP:11">
      <description>i.MX RT1180</description>
      <device Dname="MIMXRT1187xxxxx">
        <processor Pname="cm33" Dcore="Cortex-M33" Dfpu="SP_FPU" Dmpu="MPU" Dtz="TZ" Ddsp="DSP" Dendian="Little-endian" Dclock="240000000"/>
        <processor Pname="cm7" Dcore="Cortex-M7" Dfpu="DP_FPU" Dmpu="MPU" Dtz="NO_TZ" Ddsp="NO_DSP" Dendian="Little-endian" Dclock="800000000"/>
        <environment Pname="cm33" name="iar">
          <file category="linkerfile" name="devices/MIMXRT1187/iar/MIMXRT1187Axxxx_cm33_ram.icf" condition="allOf.toolchains=iar, cores=cm33, device_ids=MIMXRT1187xxxxx, boards=evkmimxrt1180a.condition_id"/>
          <file category="linkerfile" name="devices/MIMXRT1187/iar/MIMXRT1187xxxxx_cm33_ram.icf" condition="allOf.toolchains=iar, cores=cm33, device_ids=MIMXRT1187xxxxx, boards=evkmimxrt1180.condition_id"/>
        </environment>
        <environment Pname="cm7" name="iar">
          <file category="linkerfile" name="devices/MIMXRT1187/iar/MIMXRT1187Axxxx_cm7_ram.icf" condition="allOf.toolchains=iar, cores=cm7f, device_ids=MIMXRT1187xxxxx, boards=evkmimxrt1180a.condition_id"/>
          <file category="linkerfile" name="devices/MIMXRT1187/iar/MIMXRT1187xxxxx_cm7_ram.icf" condition="allOf.toolchains=iar, cores=cm7f, device_ids=MIMXRT1187xxxxx, boards=evkmimxrt1180.condition_id"/>
        </environment>
        <sequences>
          <sequence Pname="cm33" name="HaltCM33">
            <block>
    </block>
          </sequence>
          <sequence Pname="cm7" name="HaltCM33">
            <block>
      __var CM33_DHCSR_F = 0;
      __var CM33_DHCSR_S = 0;
      __var boot_mode = 0;

      // switch to CM33 AP port first
      __dp = 0;
      __ap = 3;
      
      boot_mode = (Read32(0x54460044) &gt;&gt; 24) &amp; 0x3F;  //SRC-&gt;SBMR2
    </block>
            <control if="(boot_mode == 8) || (boot_mode == 9)">
              <block>
        Message(0, "Boot from fuse or serial download mode, check if CM33 is halted...");
        CM33_DHCSR_F = Read32(0xE000EDF0);  //DHCSR
      </block>
              <control if="(CM33_DHCSR_F &amp; 0x02) == 0">
                <block>
          Message(0, "CM33 is not halted, trying to halt it. CM33 DHCSR = 0x%x", CM33_DHCSR_F);
          Write32(0xE000EDF0, 0xA05F0001);   // DHCSR, Enable CM33 debug control
          Write32(0xE000EDF0, 0xA05F0003);   // DHCSR, Halt CM33
          CM33_DHCSR_S = Read32(0xE000EDF0);  //DHCSR
        </block>
                <control if="(CM33_DHCSR_S &amp; 0x02) != 0">
                  <block>
            Message(0, "CM33 is halted now. CM33 DHCSR = 0x%x", CM33_DHCSR_S);
          </block>
                </control>
                <control if="(CM33_DHCSR_S &amp; 0x02) == 0">
                  <block>
            Message(0, "CM33 is still running, halt failed. CM33 DHCSR = 0x%x", CM33_DHCSR_S);
          </block>
                </control>
              </control>
              <control if="(CM33_DHCSR_F &amp; 0x02) != 0">
                <block>
          Message(0, "CM33 is halted already. CM33 DHCSR = 0x%x", CM33_DHCSR_F);
        </block>
              </control>
            </control>
            <control if="(boot_mode != 8) &amp;&amp; (boot_mode != 9)">
              <block>
        Message(0, "Flash execution mode, leave CM33 run status as it was...");
      </block>
            </control>
            <block>
      // switch back to CM7 AP port
      __ap = 2;
    </block>
          </sequence>
          <sequence name="InitCM7TCM">
            <block>
      __var reg = 0;
      __var targetAddr = 0;

      // DMA channel is active, wait it get finished
      reg = Read32(0x52010000);         // DMA4-&gt;TDC[0].CH_CSR
    </block>
            <control if="(reg &amp; 0x80000000) != 0">
              <control while="(Read32(0x52010000) &amp; 0x40000000) == 0" timeout="1000000"/>
            </control>
            <block>
      targetAddr = 0x303C0000;
      Write32(0x52010000, 0x40000000);    // DMA4-&gt;TDC[0].CH_CSR, clear DONE flag

      Write32(0x5201002C, 0x00000000);    // DMA4-&gt;TCD[0].SLAST_SGA
      Write32(0x52010038, 0x00000000);    // DMA4-&gt;TCD[0].DLAST_SGA

      Write32(0x52010000, 0x40000000);    // DMA4-&gt;TCD[0].CH_CSR

      Write32(0x52010020, 0x20484000);    // DMA4-&gt;TCD[0].SADDR
      Write32(0x52010030, targetAddr);    // DMA4-&gt;TCD[0].DADDR
      Write32(0x52010028, 0x40000);       // DMA4-&gt;TCD[0].NBYTES_MLOFFNO
      Write16(0x52010036, 0x1);           // DMA4-&gt;TCD[0].ELINKNO
      Write16(0x5201003E, 0x1);           // DMA4-&gt;TCD[0].BITER_ELINKNO
      Write16(0x52010026, 0x0303);        // DMA4-&gt;TCD[0].ATTR
      Write16(0x52010024, 0x0);           // DMA4-&gt;TCD[0].SOFF
      Write16(0x52010034, 0x8);           // DMA4-&gt;TCD[0].DOFF
      Write32(0x52010000, 0x7);           // DMA4-&gt;TDC[0].CH_CSR
      Write16(0x5201003C, 0x8);           // DMA4-&gt;TCD[0].CSR
      Write16(0x5201003C, 0x9);           // DMA4-&gt;TCD[0].CSR
    </block>
            <control while="(Read32(0x52010000) &amp; 0x40000000) == 0" timeout="1000000"/>
            <block>
        Write32(0x52010000, 0x40000000);    // DMA4-&gt;TDC[0].CH_CSR, clear DONE flag
    </block>
            <block>
      targetAddr = 0x30400000;
      Write32(0x52010000, 0x40000000);    // DMA4-&gt;TDC[0].CH_CSR, clear DONE flag

      Write32(0x5201002C, 0x00000000);    // DMA4-&gt;TCD[0].SLAST_SGA
      Write32(0x52010038, 0x00000000);    // DMA4-&gt;TCD[0].DLAST_SGA

      Write32(0x52010000, 0x40000000);    // DMA4-&gt;TCD[0].CH_CSR

      Write32(0x52010020, 0x20484000);    // DMA4-&gt;TCD[0].SADDR
      Write32(0x52010030, targetAddr);    // DMA4-&gt;TCD[0].DADDR
      Write32(0x52010028, 0x40000);       // DMA4-&gt;TCD[0].NBYTES_MLOFFNO
      Write16(0x52010036, 0x1);           // DMA4-&gt;TCD[0].ELINKNO
      Write16(0x5201003E, 0x1);           // DMA4-&gt;TCD[0].BITER_ELINKNO
      Write16(0x52010026, 0x0303);        // DMA4-&gt;TCD[0].ATTR
      Write16(0x52010024, 0x0);           // DMA4-&gt;TCD[0].SOFF
      Write16(0x52010034, 0x8);           // DMA4-&gt;TCD[0].DOFF
      Write32(0x52010000, 0x7);           // DMA4-&gt;TDC[0].CH_CSR
      Write16(0x5201003C, 0x8);           // DMA4-&gt;TCD[0].CSR
      Write16(0x5201003C, 0x9);           // DMA4-&gt;TCD[0].CSR
    </block>
            <control while="(Read32(0x52010000) &amp; 0x40000000) == 0" timeout="1000000"/>
            <block>
        Write32(0x52010000, 0x40000000);    // DMA4-&gt;TDC[0].CH_CSR, clear DONE flag
    </block>
          </sequence>
          <sequence Pname="cm33" name="StartCM7">
            <block>
    </block>
          </sequence>
          <sequence Pname="cm7" name="StartCM7">
            <block>
      __var resp1 = 0;
      __var resp2 = 0;
      __var reg = 0;

      // switch to CM33 AP port first
      __dp = 0;
      __ap = 3;
      
      reg = Read32(0x544F0080);  // BLK_CTRL_S_AONMIX-&gt;M7_CFG
    </block>
            <control if="(reg &amp; 0x10) == 0">
              <block>
        Message(0, "CM7 is running already");
      </block>
            </control>
            <control if="(reg &amp; 0x10) == 0x10">
              <block>
        Message(0, "Start to kickoff CM7");

        // Clock Preparation
        Write32(0x54484350, 0x0);     // ROSC400M_CTRL1
        Write32(0x54450000, 0x100);   // CLOCK_ROOT[0].CONTROL, CM7

        // VTOR 0x00
        Write32(0x544F0080, 0x0010);  // BLK_CTRL_S_AONMIX-&gt;M7_CFG

        // Release CM7
        Write32(0x54460010, 0x1);     // SRC_GENERAL_REG-&gt;SCR

        // DMA initialization
        Sequence("InitCM7TCM");

        // Making Landing Zone
        Write32(0x303C0000, 0x20000000);
        Write32(0x303C0004, 0x00000009);
        Write32(0x303C0008, 0xE7FEE7FE);

        // Trigger ELE
        Write32(0x57540200, 0x17d20106);  // MU_RT_S3MUA-&gt;TR[0]
        resp1 = Read32(0x57540280);       // MU_RT_S3MUA-&gt;RR[1]
        resp2 = Read32(0x57540284);       // MU_RT_S3MUA-&gt;RR[1]
        Message(0, "RESP1 = 0x%x", resp1);  
        Message(0, "RESP2 = 0x%x", resp2);  

        // Deassert CM7 Wait
        Write32(0x544F0080, 0x0);  // BLK_CTRL_S_AONMIX-&gt;M7_CFG
        
        Message(0, "CM7 is kicked-off");
      </block>
            </control>
            <block>
      // switch back to CM7 AP port
      __ap = 2;
    </block>
          </sequence>
          <sequence name="DebugPortStart">
            <block>
      __var SW_DP_ABORT  = 0x0;
      __var DP_CTRL_STAT = 0x4;
      __var DP_SELECT    = 0x8;
      __var powered_down = 0;
      // Switch to DP Register Bank 0
      WriteDP(DP_SELECT, 0x00000000);
      // Read DP CTRL/STAT Register and check if CSYSPWRUPACK and CDBGPWRUPACK bits are set
      powered_down = ((ReadDP(DP_CTRL_STAT) &amp; 0xA0000000) != 0xA0000000);
    </block>
            <control if="powered_down">
              <block>
        // Request Debug/System Power-Up
        WriteDP(DP_CTRL_STAT, 0x50000000);
      </block>
              <!-- Wait for Power-Up Request to be acknowledged -->
              <control while="(ReadDP(DP_CTRL_STAT) &amp; 0xA0000000) != 0xA0000000" timeout="1000000"/>
              <!-- JTAG Specific Part of sequence -->
              <control if="(__protocol &amp; 0xFFFF) == 1">
                <block>
          // Init AP Transfer Mode, Transaction Counter, and Lane Mask (Normal Transfer Mode, Include all Byte Lanes)
          // Additionally clear STICKYORUN, STICKYCMP, and STICKYERR bits by writing '1'
          WriteDP(DP_CTRL_STAT, 0x50000F32);
        </block>
              </control>
              <!-- SWD Specific Part of sequence -->
              <control if="(__protocol &amp; 0xFFFF) == 2">
                <block>
          // Init AP Transfer Mode, Transaction Counter, and Lane Mask (Normal Transfer Mode, Include all Byte Lanes)
          WriteDP(DP_CTRL_STAT, 0x50000F00);
          
          // Clear WDATAERR, STICKYORUN, STICKYCMP, and STICKYERR bits of CTRL/STAT Register by write to ABORT register
          WriteDP(SW_DP_ABORT, 0x0000001E);
        </block>
              </control>
            </control>
            <block>
       Sequence("HaltCM33");
       Sequence("StartCM7");
    </block>
          </sequence>
        </sequences>
        <memory name="ROMCP" start="0x00000000" size="0x028000" access="rx" default="1"/>
        <memory name="SRAM_OC1" start="0x20484000" size="0x07c000" access="rw" default="1"/>
        <memory name="SRAM_OC2" start="0x20500000" size="0x040000" access="rw" default="1"/>
        <algorithm name="devices/MIMXRT1189/arm/MIMXRT1180_EVK_FSPI1_QSPI_CM33.FLM" start="0x28000000" size="0x1000000" RAMstart="0x20000000" RAMsize="0x20000" default="1"/>
        <algorithm name="devices/MIMXRT1189/arm/MIMXRT1180_EVK_FSPI1_QSPI_CM7.FLM" start="0x28000000" size="0x1000000" RAMstart="0x20000000" RAMsize="0x20000" default="1"/>
        <debug svd="devices/MIMXRT1187/MIMXRT1187_cm33.xml" Pname="cm33" __dp="0" __ap="3"/>
        <debug svd="devices/MIMXRT1187/MIMXRT1187_cm7.xml" Pname="cm7" __dp="0" __ap="2"/>
        <variant Dvariant="MIMXRT1187CVM8B">
