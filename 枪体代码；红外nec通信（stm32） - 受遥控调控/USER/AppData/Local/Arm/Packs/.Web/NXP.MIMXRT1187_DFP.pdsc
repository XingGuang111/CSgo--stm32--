          <compile Pname="cm7" header="devices/MIMXRT1187/fsl_device_registers.h" define="CPU_MIMXRT1187CVM8B_cm7"/>
        </variant>
        <variant Dvariant="MIMXRT1187AVM8B">
          <compile Pname="cm33" header="devices/MIMXRT1187/fsl_device_registers.h" define="CPU_MIMXRT1187AVM8B_cm33"/>
          <compile Pname="cm7" header="devices/MIMXRT1187/fsl_device_registers.h" define="CPU_MIMXRT1187AVM8B_cm7"/>
        </variant>
        <variant Dvariant="MIMXRT1187XVM8B">
          <compile Pname="cm33" header="devices/MIMXRT1187/fsl_device_registers.h" define="CPU_MIMXRT1187XVM8B_cm33"/>
          <compile Pname="cm7" header="devices/MIMXRT1187/fsl_device_registers.h" define="CPU_MIMXRT1187XVM8B_cm7"/>
        </variant>
      </device>
    </family>
